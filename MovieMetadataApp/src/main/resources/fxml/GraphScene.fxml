<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="680.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.imdb.controller.GraphController">
   <children>
      <VBox prefHeight="680.0" prefWidth="1024.0">
         <children>
            <HBox prefHeight="1.0" prefWidth="1024.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="Movie's graph">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" right="810.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Button fx:id="btnGraphBackLanding" mnemonicParsing="false" onMouseClicked="#backLandingOnClick" text="&lt;&lt; back">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <PieChart fx:id="piechart" prefHeight="640.0" prefWidth="1024.0" title="Movie's Gross" />
         </children>
      </VBox>
   </children>
</AnchorPane>
